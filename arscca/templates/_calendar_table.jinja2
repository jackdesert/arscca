    <!-- <h2 style='color: red; font-weight: bold;'>YOUR MESSAGE HERE</h2> -->
<table>
    <tr>
        <th class='calendar-event__th'>Date</th>
        <th class='calendar-event__th'>Event</th>
        <th class='calendar-event__th'>Information</th>
        <th class='calendar-event__th'>Location</th>
    </tr>
    {% for event in events %}
        <tr class='calendar-event__tr calendar-event__tr_{{ event.css_class }}'>
            <td class='calendar-event__td'>{{ event.date_formatted  }}</td>
            <td class='calendar-event__td'>
                <div>
                    {% if event.link %}
                        <a href='{{ event.link }}' >{{ event.name }}</a>
                    {% else %}
                        {{ event.name }}
                    {% endif %}
                </div>
            </td>
            <td class='calendar-event__td'>
                {{ event.info | safe }}
            </td>
            <td class='calendar-event__td'>
                {% if event.stuttgart %}
                    <a href='//arscca.org/stuttgart'>Stuttgart Municipal</a>
                {% elif event.jitsi %}
                    <!--
                    <a href='//meet.jit.si/arscca'>Jitsi Teleconference</a>
                    -->
                    <a href='//teams.microsoft.com/l/meetup-join/19%3ameeting_ZGNiZmM3MGMtNGJmMy00YjVhLWJlYzEtNjhmYjU1ZmNjYmM4%40thread.v2/0?context=%7b%22Tid%22%3a%228e06ce42-6d5c-4065-837f-ba25c7a2a040%22%2c%22Oid%22%3a%22294696b6-5bc1-4ed3-be0a-0b49bfd4ba7b%22%7d'>Microsoft Teams Meeting</a>
                {% else %}
                    {{ event.venue }}
                    <br>
                    {{ event.address }}
                {% endif %}
            </td class='calendar-event__td'>
        </tr>
    {% endfor %}
</table>
